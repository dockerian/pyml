    file: swagger.yaml
    icon: favicon.ico
    path: apidoc
    version: {{__API_VERSION__}}
  name: {{__PROJECT_TITLE__}}
  desc: {{__PROJECT_DESCRIPTION__}}...
  host: 0.0.0.0
  port: {{__DOCKER_PORT__}}
